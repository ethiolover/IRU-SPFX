{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "tabs-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/tabs/TabsWebPart.js",
          "manifest": "./src/webparts/tabs/TabsWebPart.manifest.json"
        }
      ]
    },
    "accordion-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/accordion/AccordionWebPart.js",
          "manifest": "./src/webparts/accordion/AccordionWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "sp-client-custom-fields": "node_modules/sp-client-custom-fields/dist/sp-client-custom-fields.bundle.js",
    "jquery": {
      "path": "node_modules/jquery/dist/jquery.min.js",
      "globalName": "jQuery"
    }
  },
  "localizedResources": {
    "sp-client-custom-fields/strings": "node_modules/sp-client-custom-fields/lib/loc/{locale}.js",
    "TabsWebPartStrings": "lib/webparts/tabs/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "AccordionWebPartStrings": "lib/webparts/accordion/loc/{locale}.js"
  }
}
